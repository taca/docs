{
    "gitbook": "3.2.x",
    "title": "Manual for Contao Manager",
    "plugins": [
        "bootstrap-callout",
        "edit-link",
        "anchorjs",
        "disqus",
        "piwik",
        "toggle-chapters",
        "image-captions",
        "canonical-link"
    ],
    "links": {
      "sidebar": {
        "Contao Homepage": "https://contao.org",
        "Contao Documentation": "https://docs.contao.org"
      }
    },
    "pluginsConfig": {
        "edit-link": {
          "base": "https://github.com/contao/docs/edit/master/manager",
            "label": {
                "en": "Edit This Page",
                "de": "Seite bearbeiten"
            }
        },
        "disqus": {
            "shortName": "contaodocs",
            "useIdentifier": true
        },
        "piwik": {
            "URL": "association.contao.org/piwik/",
            "siteId": 3
        },
        "canonical-link": {
            "baseURL": "https://docs.contao.org/books/manager/en/"
        },
        "anchorjs": {
            "selector": "h1,h2,h3,*:not(.callout) > h4,h5"
        }
    }
}
