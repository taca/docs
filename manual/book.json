{
    "gitbook": "3.2.x",
    "title": "Manual for Contao 4.4",
    "plugins": [
        "bootstrap-callout",
        "edit-link",
        "anchorjs",
        "disqus",
        "piwik",
        "toggle-chapters",
        "image-captions",
        "versions-select",
        "canonical-link"
    ],
    "links": {
      "sidebar": {
        "Contao Homepage": "https://contao.org",
        "Contao Documentation": "https://docs.contao.org"
      }
    },
    "pluginsConfig": {
        "edit-link": {
          "base": "https://github.com/contao/docs/edit/4.4/manual",
            "label": {
                "en": "Edit This Page",
                "de": "Seite bearbeiten",
                "fr": "Modifier cette page"
            }
        },
        "disqus": {
            "shortName": "contaodocs",
            "useIdentifier": true
        },
        "piwik": {
            "URL": "association.contao.org/piwik/",
            "siteId": 3
        },
        "versions": {
            "gitbookConfigURL": "https://raw.githubusercontent.com/contao/docs/master/manual/book.json",
            "options": [
                { "value": "https://docs.contao.org/books/manual/4.4", "text": "Contao 4.4" },
                { "value": "https://docs.contao.org/books/manual/4.3", "text": "Contao 4.3" },
                { "value": "https://docs.contao.org/books/manual/4.2", "text": "Contao 4.2" },
                { "value": "https://docs.contao.org/books/manual/4.0", "text": "Contao 4.0" },
                { "value": "https://docs.contao.org/books/manual/3.5", "text": "Contao 3.5" },
                { "value": "https://docs.contao.org/books/manual/3.4", "text": "Contao 3.4" },
                { "value": "https://docs.contao.org/books/manual/3.3", "text": "Contao 3.3" },
                { "value": "https://docs.contao.org/books/manual/3.2", "text": "Contao 3.2" },
                { "value": "https://docs.contao.org/books/manual/3.1", "text": "Contao 3.1" },
                { "value": "https://docs.contao.org/books/manual/3.0", "text": "Contao 3.0" },
                { "value": "https://docs.contao.org/books/manual/2.11", "text": "Contao 2.11" }
            ]
        },
        "canonical-link": {
            "baseURL": "https://docs.contao.org/books/manual/current/en/"
        },
        "anchorjs": {
            "selector": "h1,h2,h3,*:not(.callout) > h4,h5"
        }
    }
}
